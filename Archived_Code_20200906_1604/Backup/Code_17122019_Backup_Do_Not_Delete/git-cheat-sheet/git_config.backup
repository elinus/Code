[core]
    autoclrf = input
	filemode = false
    compression = 9
[remote "origin"]
	url = https://github.com/elinus/Code.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[user]
	email = ayruomlinus@gmail.com
	name = Sunil Mourya
	username = elinus
[branch "master"]
	remote = origin
	merge = refs/heads/master
[merge]
	tool = vimdiff
[color]
    ui = auto
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[push]
    default = simple
[credential]
	helper = cache --timeout 7200
